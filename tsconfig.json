{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "noEmit": false,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": ["./"]
}

